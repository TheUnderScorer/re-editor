{
  "rules": {
    "semi": [
      "error",
      "always"
    ],
    "no-unused-vars": "warn",
    "no-useless-escape": "warn",
    "no-unused-labels": "warn"
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ]
}